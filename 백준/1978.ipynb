   "source": [
    "N = int(input())\n",
    "nums = list(map(int,input().split()))\n",
    "\n",
    "for num in nums:\n",
    "    if num == 1:\n",
    "        N -=1\n",
    "    else:\n",
    "        for j in range(2, num):\n",
    "            if num%j == 0:\n",
    "                N -=1\n",
    "                break\n",
    "            else:\n",
    "                continue\n",
    "print(N)\n"
   ]
  }
